        name: de
        url: https://pokeapi.co/api/v2/language/6/
      version_group:
        name: lets-go-pikachu-lets-go-eevee
        url: https://pokeapi.co/api/v2/version-group/19/
    - flavor_text: "Usando la tuerca del pecho como eje, el usuario gira\nsobre s\xED
