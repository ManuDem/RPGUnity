      capture_rate: 60
      flavor_text_entries:
      - flavor_text: "\u3072\u3068\u3092\u3000\u3042\u3084\u3064\u308A\u3000\u3059\u306A\u306E\u3084\u307E\u3092\n\u3059\u306A\u306E\u3057\u308D\u307E\u3067\u3000\u3057\u3093\u304B\u3000\u3055\u305B\u305F\u3002\n\u306E\u308D\u3044\u306E\u3000\u3061\u304B\u3089\u3082\u3000\u3042\u304C\u3063\u3066\u3044\u308B\u3002"
        language:
          name: ja-Hrkt
          url: https://pokeapi.co/api/v2/language/1/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "\uC0AC\uB78C\uC744 \uC870\uC885\uD558\uC5EC \uBAA8\uB798\uC5B8\uB355\uC744\n\uBAA8\uB798\uC131\uC73C\uB85C\uAE4C\uC9C0
          \uC9C4\uD654\uC2DC\uCF30\uB2E4.\n\uC800\uC8FC\uC758 \uD798\uB3C4 \uAE4A\uC5B4\uC84C\uB2E4."
        language:
          name: ko
          url: https://pokeapi.co/api/v2/language/3/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "\u64CD\u7E31\u4EBA\u5011\uFF0C\n\u8B93\u6C99\u5C71\u9032\u5316\u70BA\u6C99\u5821\u3002\n\u8A5B\u5492\u7684\u529B\u91CF\u4E5F\u5F97\u5230\u4E86\u63D0\u5347\u3002"
        language:
          name: zh-Hant
          url: https://pokeapi.co/api/v2/language/4/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "Il a poss\xE9d\xE9 un humain pour que ce dernier\nle remod\xE8le
          en ch\xE2teau de sable, d\xE9cuplant\nau passage ses pouvoirs de mal\xE9diction."
        language:
          name: fr
          url: https://pokeapi.co/api/v2/language/5/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "Brachte Menschen dazu, den Sandh\xFCgel zu einer\nstattlichen
          Sandburg auszubauen. Auch seine\nFl\xFCche haben an St\xE4rke gewonnen."
        language:
          name: de
          url: https://pokeapi.co/api/v2/language/6/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: 'Manipulando a la gente ha conseguido

          transformarse
          en todo un castillo de arena,

          lo que ha aumentado su poder maligno.'
        language:
          name: es
          url: https://pokeapi.co/api/v2/language/7/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "Si \xE8 evoluto servendosi degli esseri umani per\ndiventare
          un castello di sabbia, aumentando\ncos\xEC la propria forza malefica."
        language:
          name: it
          url: https://pokeapi.co/api/v2/language/8/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "Possessed people controlled by this Pok\xE9mon\ntransformed
          its sand mound into a castle. As it\nevolved, its power to curse grew ever
          stronger."
        language:
          name: en
          url: https://pokeapi.co/api/v2/language/9/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "\u4EBA\u3092\u3000\u64CD\u308A\u3000\u7802\u306E\u5C71\u3092\n\u7802\u306E\u57CE\u307E\u3067\u3000\u9032\u5316\u3000\u3055\u305B\u305F\u3002\n\u546A\u3044\u306E\u3000\u529B\u3082\u3000\u3042\u304C\u3063\u3066\u3044\u308B\u3002"
        language:
          name: ja
          url: https://pokeapi.co/api/v2/language/11/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "\u64CD\u7EB5\u4EBA\u4EEC\uFF0C\n\u8BA9\u6C99\u4E18\u8FDB\u5316\u4E3A\u6C99\u5821\u3002\n\u8BC5\u5492\u7684\u529B\u91CF\u4E5F\u5F97\u5230\u4E86\u63D0\u5347\u3002"
        language:
          name: zh-Hans
          url: https://pokeapi.co/api/v2/language/12/
        version:
          name: sun
          url: https://pokeapi.co/api/v2/version/27/
      - flavor_text: "\u3057\u308D\u306E\u3000\u3057\u305F\u306B\u306F\u3000\u305B\u3044\u304D\u3092\u3000\u3059\u308F\u308C\n\u3072\u304B\u3089\u3073\u305F\u3000\u3082\u306E\u305F\u3061\u306E\u3000\u30DB\u30CD\u304C\n\u305F\u3044\u308A\u3087\u3046\u306B\u3000\u3046\u307E\u3063\u3066\u3044\u308B\u3002"
        language:
          name: ja-Hrkt
          url: https://pokeapi.co/api/v2/language/1/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: "\uC131 \uC544\uB798\uC5D0\uB294 \uC815\uAE30\uB97C \uBE68\uB824\n\uBC14\uC2F9
          \uB9D0\uB77C\uBC84\uB9B0 \uC790\uB4E4\uC758 \uBF08\uAC00\n\uB300\uB7C9\uC73C\uB85C
          \uBB3B\uD600\uC788\uB2E4."
        language:
          name: ko
          url: https://pokeapi.co/api/v2/language/3/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: "\u5728\u57CE\u5821\u7684\u4E0B\u9762\u57CB\u85CF\u8457\u5927\u91CF\n\u88AB\u5438\u4E7E\u4E86\u7CBE\u6C23\u8005\u7684\u9AB8\u9AA8\u3002"
        language:
          name: zh-Hant
          url: https://pokeapi.co/api/v2/language/4/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: "On trouve de nombreux ossements sous\nses tours, restes de
          pauvres h\xE8res captur\xE9s.\nLeur \xE9nergie a servi \xE0 nourrir ce
          Pok\xE9mon."
        language:
          name: fr
          url: https://pokeapi.co/api/v2/language/5/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: 'Unter seiner Sandburg liegen zahlreiche

          Knochen
          qualvoll Verstorbener, denen

          die Lebenskraft ausgesaugt wurde.'
        language:
          name: de
          url: https://pokeapi.co/api/v2/language/6/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: "Su castillo alberga abundantes huesos de todos\naquellos incautos
          a los que absorbi\xF3 su vitalidad\npor completo antes de quedar sepultados."
        language:
          name: es
          url: https://pokeapi.co/api/v2/language/7/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: 'Succhia energia vitale dalle vittime e ne

          seppellisce
          le ossa rinsecchite sotto il castello.'
        language:
          name: it
          url: https://pokeapi.co/api/v2/language/8/
        version:
          name: moon
          url: https://pokeapi.co/api/v2/version/28/
      - flavor_text: 'Buried beneath the castle are masses of

          dried-up
          bones from those whose vitality

          it has drained.'
