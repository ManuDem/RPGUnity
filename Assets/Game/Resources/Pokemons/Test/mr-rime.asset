        language:
          name: es
          url: https://pokeapi.co/api/v2/language/7/
        version:
          name: sword
          url: https://pokeapi.co/api/v2/version/33/
      - flavor_text: "\xC8 un maestro del tip tap. Fa roteare il suo\nbastone di
