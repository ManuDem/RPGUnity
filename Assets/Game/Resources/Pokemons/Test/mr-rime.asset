        version:
          name: sword
          url: https://pokeapi.co/api/v2/version/33/
      - flavor_text: "\u8E22\u8E0F\u821E\u7684\u9054\u4EBA\u3002\n\u6703\u63EE\u52D5\u624B\u4E2D\u7684\u51B0\u6756\uFF0C\n\u5411\u89C0\u773E\u8868\u6F14\u8F15\u5FEB\u7684\u821E\u6B65\u3002"
