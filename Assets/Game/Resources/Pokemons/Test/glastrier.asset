          name: de
          url: https://pokeapi.co/api/v2/language/6/
        version:
          name: sword
          url: https://pokeapi.co/api/v2/version/33/
      - flavor_text: "Libera un potente vaho g\xE9lido por las pezu\xF1as.\nUn ser
