          url: https://pokeapi.co/api/v2/language/5/
        version:
          name: lets-go-pikachu
          url: https://pokeapi.co/api/v2/version/31/
      - flavor_text: "Einst wurde es f\xFCr seine F\xE4higkeit verehrt, Eisen\nerschaffen
