        language:
          name: es
          url: https://pokeapi.co/api/v2/language/7/
        version:
          name: lets-go-pikachu
          url: https://pokeapi.co/api/v2/version/31/
      - flavor_text: "In passato era venerato perch\xE9 lo si riteneva\ncapace di
